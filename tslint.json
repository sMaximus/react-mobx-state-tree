{
  "extends": ["tslint-config-strictish"],
  "rules": {
    "member-access": false,
    "typedef": false,
    "eofline": false,
    "no-unused-expression": "off",
    "@typescript-eslint/no-unused-expressions": ["error"],
    "allowShortCircuit" :true,
    "allowTernary" : true,
    "allowTaggedTemplates" : true
    
  }
}